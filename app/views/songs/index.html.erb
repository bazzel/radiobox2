<h1>Listing songs</h1>

<table>
  <thead>
    <tr>
      <th>Channel</th>
      <th>Title</th>
      <th>Artist</th>
      <th>Finished</th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.channel %></td>
        <td><%= song.title %></td>
        <td><%= song.artist %></td>
        <td><%= song.stopdatetime %></td>
      </tr>
    <% end %>
  </tbody>
</table>
